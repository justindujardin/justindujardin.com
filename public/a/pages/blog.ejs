<%
for(key in public.a.articles._data) {
  var article = public.a.articles._data[key];
%>
<section class="blog-summary">
  <header>
    <h1><a href="<%- url %>a/articles/<%- key %>.html"><%- article.title %></a></h1>
  </header>
  <footer>
    <p><%- article.blurb || '' %></p>
  </footer>
</section>
<% } %>
